{
	"compilerOptions": {
		"esModuleInterop": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"module": "ESNext",
		"moduleResolution": "Node",
		"noEmit": true,
		"noImplicitThis": true,
		"resolveJsonModule": true,
		"strictNullChecks": true,
		"target": "ESNext"
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"]
}
